{
  "name": "gen-vis",
  "displayName": "gen-vis",
  "description": "A VSCode extension for visualizing data produced by a QuickCheck generator.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:haskell",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gen-vis.view-visualization",
        "title": "View Visualization",
        "category": "Generator Visualizer"
      },
      {
        "command": "gen-vis.pick-new-data",
        "title": "Choose New Visualization Data",
        "category": "Generator Visualizer"
      },
      {
        "command": "gen-vis.refresh-data",
        "title": "Refresh Visualization Data",
        "category": "Generator Visualizer"
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "test:webview": "cd webview-ui && npm run test",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "sass": "^1.56.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
